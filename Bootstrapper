import random

numbers = [1,5,2,2,20,20,5,3]
totalcycles = 20

avg = sum(numbers) / len(numbers)
print ("Original Dataset:")
print (numbers)
print ("\nMean: %s \n" % (avg))

def boot_strapper():
    cycles = totalcycles
    means = []
    print ("Randomized Datasets:")
    while cycles > 0:
        setsize = len(numbers)
        dataset = []
        while setsize > 0:
            rand = random.choice(numbers)
            dataset.append(rand)
            setsize -= 1
        mean = (sum(dataset) / len(numbers))
        means.append(mean)
        cycles -= 1
        print (dataset)
    print ("\nGenerated Means:")
    print (means)
    result = sum(means) / len(means)
    print ("\nBootstrapped Mean:")
    print (result)

print (boot_strapper())
